   },
   "outputs": [],
   "source": [
    "trie  = {}\n",
    "\n",
    "def in_trie(str):\n",
    "    obj = trie\n",
    "    for char in str:\n",
    "        if char not in obj:\n",
    "            obj[char] = {}\n",
    "            obj = obj[char]\n",
    "        elif not obj[char]:\n",
    "            return True\n",
    "        else:\n",
    "            obj = obj[char]\n",
    "    return bool(obj)"
   ]
